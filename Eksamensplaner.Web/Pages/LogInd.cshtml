@page
@model PrivacyModel
@{
    ViewData["Title"] = "AdminLogInd";
}

<!DOCTYPE html>
<html lang="da">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/LogInd.css" asp-append-version="true" />
</head>

<body>
<section class="login-section">
    <h1 class="login-container">
        
        <div class="login-box">
            <h2>Studerende</h2>
            
            <form method="post" action="/Student/Schedule">
                <label for="student-username">Brugernavn</label>
                <input type="text" id="student-username" name="StudentUserName" placeholder="Skriv brugernavn" required>
                 
                <label for="student-password">Adgangskode</label>
                <input type="password" id="student-password" name="StudentPassword" placeholder="Skriv adgangskode" required>
                        
                <button type="submit">Log ind</button>
            </form>
        </div>
        
        <div class="login-box">      **
            <h2>Underviser</h2>
            
            <form method="post" action="/Teacher/Schedule">
                <label for="teacher-username">Brugernavn</label>
                <input type="text" id="teacher-username" name="TeacherUserName" placeholder="Skriv brugernavn" required>
                    
                <label for="teacher-password">Adgangskode</label>
                <input type="password" id="teacher-password" name="TeacherPassword" placeholder="Skriv adgangskode" required>
                        
                <button type="submit">Log ind</button>
            </form>
        </div>
        
        
    </h1>
</section>

</body>